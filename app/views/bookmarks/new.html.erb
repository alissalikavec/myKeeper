<h1>New Bookmark</h1>
<%= form_for :bookmark, url: bookmarks_path do |f| %>
<p>
<%= f.label :title %><br/>
<%= f.text_field :title %>
</p>
<p>
<%= f.label :tag_list, "Tags, separated by commas:" %><br/>
<%= f.text_field :tag_list %>
</p>
<p>
<%= f.label :text %><br/>
<%= f.text_area :text %>
</p>

<p>
<%= f.submit %>
</p>

<% end %>